
note <- sprintf("R package version %s", meta$Version)

textVersion = paste0("Adam H. Sparks and Emerson Del Ponte. (2019). What's so Open About Plant Pathology? version 0.0.10. Australian National University Open Science Day. 2019-02-25, https://openplantpathology.github.io/OPP.at.ANU/")

bibentry(bibtype = "Proceedings",
         title = " What's so Open About Plant Pathology?",
         author = c(person("Adam", "Sparks"), person("Emerson", "Del Ponte")),
         booktitle = "Australian National University Open Science Day",
         address = "Canberra, Australia",
         publisher = "Australian National University",
         year = 2019,
         note = note,
         url = "https://openplantpathology.github.io/OPP.at.ANU/")
